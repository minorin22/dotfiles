{
  "$schema": "https://zed.dev/schema/themes/v0.2.0.json",
  "author": "minorin",
  "name": "Monokai Japan",
  "themes": [
    {
      "name": "Monokai Japan",
      "appearance": "dark",
      "style": {
        "background": "#272727",
        "surface.background": "#202020",
        "elevated_surface.background": "#414141",
        "panel.background": "#202020",
        "pane.focused_border": "#6b6b6b",
        "panel.focused_border": "#6b6b6b",

        "text": "#fbfbf8",
        "text.muted": "#cccccc",
        "text.placeholder": "#90908a",
        "text.disabled": "#6b6b6b",
        "text.accent": "#fb966e",

        "icon": "#fbfbf8",
        "icon.muted": "#cccccc",
        "icon.disabled": "#6b6b6b",
        "icon.placeholder": "#90908a",
        "icon.accent": "#fb966e",

        "border": "#414141",
        "border.variant": "#363636",
        "border.focused": "#6b6b6b",
        "border.selected": "#6b6b6b",
        "border.disabled": "#363636",
        "border.transparent": "#00000000",

        "element.background": "#414141",
        "element.hover": "#3f3f3f",
        "element.active": "#6b6b6b",
        "element.selected": "#6b6b6b",
        "element.disabled": "#363636",

        "drop_target.background": "#41414180",

        "status_bar.background": "#414141",
        "title_bar.background": "#202020",
        "toolbar.background": "#272727",
        "tab_bar.background": "#202020",
        "tab.inactive_background": "#363636",
        "tab.active_background": "#272727",

        "search.match_background": "#6b6b6b",

        "scrollbar.thumb.background": "#6b6b6b80",
        "scrollbar.thumb.hover_background": "#6b6b6b",
        "scrollbar.thumb.border": "#6b6b6b",
        "scrollbar.track.background": "#272727",
        "scrollbar.track.border": "#414141",

        "editor.foreground": "#fbfbf8",
        "editor.background": "#272727",
        "editor.gutter.background": "#272727",
        "editor.subheader.background": "#202020",
        "editor.active_line.background": "#3f3f3f",
        "editor.highlighted_line.background": "#3f3f3f",
        "editor.line_number": "#90908a",
        "editor.active_line_number": "#c2c2bf",
        "editor.invisible": "#474747",
        "editor.wrap_guide": "#474747",
        "editor.active_wrap_guide": "#777777",
        "editor.document_highlight.read_background": "#4a4a7680",
        "editor.document_highlight.write_background": "#6a6a9680",

        "terminal.background": "#272727",
        "terminal.foreground": "#fbfbf8",
        "terminal.bright_foreground": "#fbfbf8",
        "terminal.dim_foreground": "#cccccc",
        "terminal.ansi.black": "#434944",
        "terminal.ansi.bright_black": "#6b6b6b",
        "terminal.ansi.dim_black": null,
        "terminal.ansi.red": "#da5673",
        "terminal.ansi.bright_red": "#dba2b4",
        "terminal.ansi.dim_red": null,
        "terminal.ansi.green": "#7bb75b",
        "terminal.ansi.bright_green": "#898e38",
        "terminal.ansi.dim_green": null,
        "terminal.ansi.yellow": "#eebf35",
        "terminal.ansi.bright_yellow": "#8a6b3d",
        "terminal.ansi.dim_yellow": null,
        "terminal.ansi.blue": "#5cabdc",
        "terminal.ansi.bright_blue": "#126b8c",
        "terminal.ansi.dim_blue": null,
        "terminal.ansi.magenta": "#7457a2",
        "terminal.ansi.bright_magenta": "#b595cf",
        "terminal.ansi.dim_magenta": null,
        "terminal.ansi.cyan": "#87c7d4",
        "terminal.ansi.bright_cyan": "#44a9ba",
        "terminal.ansi.dim_cyan": null,
        "terminal.ansi.white": "#fbfbf8",
        "terminal.ansi.bright_white": "#bfc2bc",
        "terminal.ansi.dim_white": null,

        "link_text.hover": "#5cabdc",

        "conflict": "#eebf35",
        "conflict.background": "#eebf3520",
        "conflict.border": "#eebf35",
        "created": "#7bb75b",
        "created.background": "#7bb75b20",
        "created.border": "#7bb75b",
        "deleted": "#da5673",
        "deleted.background": "#da567320",
        "deleted.border": "#da5673",
        "error": "#da5673",
        "error.background": "#da567320",
        "error.border": "#da5673",
        "hidden": "#6b6b6b",
        "hidden.background": "#27272720",
        "hidden.border": "#6b6b6b",
        "hint": "#5cabdc",
        "hint.background": "#5cabdc20",
        "hint.border": "#5cabdc",
        "ignored": "#6b6b6b",
        "ignored.background": "#27272720",
        "ignored.border": "#6b6b6b",
        "info": "#5cabdc",
        "info.background": "#5cabdc20",
        "info.border": "#5cabdc",
        "modified": "#eebf35",
        "modified.background": "#eebf3520",
        "modified.border": "#eebf35",
        "predictive": "#6b6b6b",
        "predictive.background": "#27272720",
        "predictive.border": "#6b6b6b",
        "renamed": "#87c7d4",
        "renamed.background": "#87c7d420",
        "renamed.border": "#87c7d4",
        "success": "#7bb75b",
        "success.background": "#7bb75b20",
        "success.border": "#7bb75b",
        "unreachable": "#6b6b6b",
        "unreachable.background": "#27272720",
        "unreachable.border": "#6b6b6b",
        "warning": "#eebf35",
        "warning.background": "#eebf3520",
        "warning.border": "#eebf35",

        "syntax": {
          "comment":              { "color": "#88846f", "font_style": "italic" },
          "string":               { "color": "#eebf35" },
          "string.escape":        { "color": "#dba2b4" },
          "keyword":              { "color": "#dba2b4" },
          "keyword.operator":     { "color": "#dba2b4" },
          "operator":             { "color": "#dba2b4" },
          "function":             { "color": "#7bb75b" },
          "function.builtin":     { "color": "#87c7d4" },
          "variable":             { "color": "#fbfbf8" },
          "variable.builtin":     { "color": "#fb966e" },
          "variable.parameter":   { "color": "#fb966e", "font_style": "italic" },
          "type":                 { "color": "#7bb75b" },
          "type.builtin":         { "color": "#87c7d4", "font_style": "italic" },
          "constructor":          { "color": "#7bb75b" },
          "constant":             { "color": "#b595cf" },
          "constant.builtin":     { "color": "#b595cf" },
          "number":               { "color": "#b595cf" },
          "boolean":              { "color": "#b595cf" },
          "punctuation":          { "color": "#fbfbf8" },
          "property":             { "color": "#fbfbf8" },
          "attribute":            { "color": "#7bb75b" },
          "tag":                  { "color": "#dba2b4" },
          "label":                { "color": "#7bb75b" },
          "namespace":            { "color": "#7bb75b" },
          "embedded":             { "color": "#fbfbf8" },
          "text.literal":         { "color": "#fb966e" },
          "markup.heading":       { "color": "#7bb75b", "font_weight": 700 },
          "markup.list":          { "color": "#eebf35" },
          "markup.bold":          { "color": "#87c7d4", "font_weight": 700 },
          "markup.italic":        { "color": "#87c7d4", "font_style": "italic" },
          "markup.link":          { "color": "#eebf35" },
          "markup.quote":         { "color": "#6b6b6b", "font_style": "italic" },
          "diff.plus":            { "color": "#7bb75b" },
          "diff.minus":           { "color": "#dba2b4" },
          "diff.delta":           { "color": "#eebf35" }
        }
      }
    }
  ]
}
